                    },
                    "doc": "Dependencies to resolve.\nVersion conflict resolution may apply if necessary for the given list of dependencies.\n\n",
                    "internalAttributes": {
                        "isShorthand": true
                    }
                },
                {
                    "name": "resolvedFiles",
                    "type": {
                        "type": "list",
                        "elementType": {
                            "type": "path"
                        }
                    },
                    "doc": "Resolved classpath files.",
                    "internalAttributes": {
                        "isProvided": true
                    }
                }
