# Class with psvm should be found automatically and be default target for running module.
[module]
target = jvm
dependencies = [
    maven("org.apache.logging.log4j:log4j-api-kotlin:1.3.0"),
    # or extended form
    maven(
        group = "org.apache.logging.log4j",
        artifact = "log4j-api-kotlin",
        version = "1.3.0",
    ),
    # or even (undesirable because we can have different registries for third-party dependencies)
    "org.apache.logging.log4j:log4j-api-kotlin:1.3.0"
]
