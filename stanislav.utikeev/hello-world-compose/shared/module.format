[module]
target = multiplatform
dependencies = [
    maven("androidx.compose.ui:ui:1.2.1"),
    maven("androidx.compose.material:material:1.2.1"),
]

[multiplatform]
# Platforms for which we build. They can add plugins as well as target does.
platforms = [android, native, ios]
# Enable JetPack Compose compiler
compose = true
