[module]
# Native target defaults to user's current platform otherwise taken from native.platforms.
target = native
dependencies = [
    maven("io.github.microutils:kotlin-logging-jvm:3.0.4"),
    # or if we'll have some sort of Kotlin package registry
    "io.github.microutils:kotlin-logging-jvm:3.0.4",
]

[native]
# Any target available here: https://kotlinlang.org/docs/multiplatform-dsl-reference.html#targets
platforms = [
    macosX64,
    linuxArm64,
]
